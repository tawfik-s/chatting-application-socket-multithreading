<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane xmlns="http://javafx.com/javafx" xmlns:fx="http://javafx.com/fxml" fx:controller="org.example.controllers.CardItemController">
    <children>
        <ScrollPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
            <content>
                <VBox fx:id="cardItem" alignment="CENTER" spacing="10.0">
                    <Label fx:id="titleLabel" text="Title"/>
                    <Label fx:id="descriptionLabel" text="Description"/>
                </VBox>
            </content>
        </ScrollPane>
    </children>
</AnchorPane>